{
  "name": "tuleap/plugin-svn",
  "autoload": {
    "psr-4": {
      "Tuleap\\SVN\\": ["include/", "include/SVN/"]
    },
    "classmap": ["include/"]
  },
  "autoload-dev": {
    "psr-4": {
      "Tuleap\\SVN\\": ["./tests/unit/"]
    }
  },
  "config": {
    "sort-packages": true
  },
  "tuleap": {
    "preload": {
      "include": [
        "/include"
      ],
      "exclude": [
        "/include/SVN/AccessControl/SVNRefreshAllAccessFilesCommand.php"
      ]
    }
  }
}
