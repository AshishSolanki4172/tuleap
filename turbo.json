{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "globalDependencies": [
    "tsconfig.json",
    "tools/utils/scripts/**",
    "tools/utils/nix/**"
  ],
  "pipeline": {
    "typecheck": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "build": {
      "dependsOn": [
        "typecheck",
        "^build"
      ],
      "outputs": ["dist/**", "frontend-assets/**", "backend-assets/**"]
    },
    "test": {
      "outputs": []
    }
  }
}
