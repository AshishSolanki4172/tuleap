<div class="breadcrumb-container {{# has_only_one_breadcrumb }}breadcrumb-with-only-one-item{{/ has_only_one_breadcrumb }}">
    {{# project_context }}
        {{> breadcrumbs/breadcrumbs-privacy }}
    {{/ project_context }}
    <nav class="breadcrumb">
        {{# breadcrumbs }}
            {{^ has_sub_items }}
                {{# is_ellipsis }}
                    <span class="breadcrumb-item breadcrumb-item-disabled">
                        <span class="breadcrumb-link" title="{{ reason }}"> ... </span>
                    </span>
                {{/ is_ellipsis }}
                {{^ is_ellipsis }}
                    <span class="breadcrumb-item {{ classname }}">
                        {{# item }}
                            {{# url }}
                                <a class="breadcrumb-link"
                                   href="{{ url }}"
                                   title="{{ label }}"
                                    {{# data_attributes }}
                                   data-{{ key }}="{{ value }}"
                                    {{/ data_attributes }}
                                >
                                    {{# project_icon }}<span aria-hidden="true"> {{ . }} </span>{{/ project_icon }}{{ label }}
                                </a>
                            {{/ url }}
                        {{/ item }}
                    </span>
                {{/ is_ellipsis }}
            {{/ has_sub_items }}

            {{# has_sub_items }}
                <div class="breadcrumb-switchable breadcrumb-item {{ classname }}">
                    {{# is_ellipsis }}
                            <span class="breadcrumb-item breadcrumb-item-disabled">
                            <span class="breadcrumb-link" title="{{ reason }}"> ... </span>
                        </span>
                    {{/ is_ellipsis }}
                    {{^ is_ellipsis }}
                        {{# item }}
                            <a class="breadcrumb-link"
                               href="{{ url }}"
                               title="{{ label }}"
                                {{# data_attributes }}
                                    data-{{ key }}="{{ value }}"
                                {{/ data_attributes }}
                            >
                                {{ label }}
                            </a>
                        {{/ item }}
                    {{/ is_ellipsis }}
                    <div class="breadcrumb-switch-menu-container">
                        <nav class="breadcrumb-switch-menu">
                            {{# sections }}
                                {{# has_label }}
                                    <div class="breadcrumb-dropdown-section-title">{{ label }}</div>
                                {{/ has_label }}
                                {{^ has_label }}
                                    <div class="breadcrumb-dropdown-separator"></div>
                                {{/ has_label }}
                                {{# links }}
                                    <span class="breadcrumb-dropdown-item">
                                        <a class="breadcrumb-dropdown-link"
                                           href="{{ url }}"
                                           title="{{ label }}"
                                           {{# data_attributes }}
                                               data-{{ key }}="{{ value }}"
                                           {{/ data_attributes }}
                                        >
                                            {{ label }}
                                        </a>
                                    </span>
                                {{/ links }}
                            {{/ sections }}
                        </nav>
                    </div>
                </div>
            {{/ has_sub_items }}
        {{/ breadcrumbs }}
    </nav>
</div>
